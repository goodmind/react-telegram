{
  "name": "react-telegram",
  "version": "0.0.1",
  "description": "A react renderer for Telegram Bot API",
  "main": "./lib/index.js",
  "scripts": {
    "lint": "eslint 'src/**/*.js'",
    "watch": "babel -w ./src --out-dir ./lib",
    "build": "babel ./src --out-dir ./lib",
    "prepublish": "npm run build",
    "test": "mocha -R spec --compilers js:babel-register ./test/endpoint.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goodmind/react-telegram.git"
  },
  "keywords": [
    "telegram",
    "react",
    "renderer"
  ],
  "author": "goodmind",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/goodmind/react-telegram/issues"
  },
  "homepage": "https://github.com/goodmind/react-telegram#readme",
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.6.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "^2.4.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "dependencies": {
    "invariant": "^2.2.0",
    "lodash": "^4.17.4"
  }
}
